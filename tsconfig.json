{
  "extends": "@quasar/app-vite/tsconfig-preset.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "app/*": ["*"],
      "components/*": ["./src/components/*"],
      "layouts/*": ["./src/layouts/*"],
      "pages/*": ["./src/pages/*"],
      "assets/*": ["./src/assets/*"],
      "boot/*": ["./src/boot/*"],
      "stores/*": ["./src/stores/*"]
    }
  },
  "exclude": ["dist", "node_modules"]
}
